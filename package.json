{
  "name": "@effective/eslint-cfg",
  "version": "1.0.0",
  "description": "A library for generating ESLint Flat Configs with React and TypeScript support.",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "tsx src/index.js",
    "test": "vitest",
    "check": "tsc --noEmit",
    "lint": "eslint .",
    "fix": "eslint --fix .; prettier --write .",
    "format": "prettier --write .",
    "update": "ncu -ui",
    "release": "git push && release-it --ci patch",
    "release:minor": "git push && release-it --ci minor",
    "release:major": "git push && release-it --ci major"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "eslint-config-biome": "^1.9.4",
    "eslint-config-prettier": "^9.1.0",
    "npm-check-updates": "^17.1.11",
    "prettier": "^3.4.2",
    "release-it": "^17.10.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-n": "^17.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-compiler": "19.0.0-beta-df7b47d-20241124",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-regexp": "^2.7.0",
    "typescript-eslint": "^8.18.1"
  }
}
